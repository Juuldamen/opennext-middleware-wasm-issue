{
	"name": "open-next15",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "npm run generate-db-types && next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare && wrangler deploy",
		"preview": "opennextjs-cloudflare && wrangler dev",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"generate-db-types": "prisma generate --schema=./src/schema.prisma",
		"migrate:diff": "wrangler d1 migrations create OPENNEXT_TEST_DB rename_me && prisma migrate diff --from-local-d1 --to-schema-datamodel ./src/schema.prisma --script --output \"$(find ./migrations -type f -name '*rename_me.sql' | head -n 1)\"",
		"migrate:apply": "wrangler d1 migrations apply OPENNEXT_TEST_DB"
	},
	"dependencies": {
		"@prisma/adapter-d1": "^6.5.0",
		"@prisma/client": "^6.5.0",
		"next": "14.2.24",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250313.0",
		"@opennextjs/cloudflare": "^0.5.12",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "14.2.24",
		"prisma": "^6.5.0",
		"typescript": "^5",
		"wrangler": "^4.0.0"
	}
}
